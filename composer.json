{
    "name": "viadroid/ckeditor5",
    "description": "CKEditor 5 integration with Laravel",
    "type": "library",
    "keywords": ["laravel-admin", "extension", "ckeditor", "ckeditor5"],
    "homepage": "https://github.com/viadroid/ckeditor5",
    "license": "MIT",
    "authors": [
        {
            "name": "Leo",
            "email": "likai.qin75@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "~1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Viadroid\\CKEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Viadroid\\CKEditor\\CKEditorServiceProvider"
            ]
        }
    }
}
